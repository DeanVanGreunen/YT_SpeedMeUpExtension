{
  "manifest_version": 2,
  "name": "Youtube Playback",
  "version": "1.0",
  "description": "Manage Playback Speed With Any Value",
  "author": "https://keybase.io/DeanVanGreunen",
  "homepage_url": "https://github.com/DeanVanGreunen/YT_SpeedMeUpExtension",
  "repo_url": "https://github.com/DeanVanGreunen/YT_SpeedMeUpExtension/tree/master/src",
  "permissions": [  
    "tabs",
    "activeTab"
  ],
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "default_icon": {
      "128": "icon.png"
    },

    "default_title": "Youtube Playback",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ]
}
